%nav.my-3{'aria-label': '${1:Page navigation}'}
  %ul.pagination.justify-content-center
    %li.page-item.disabled
      %a.page-link{href: '#'} ${2:Previous}
    %li.page-item.active{'aria-current': 'page'}
      %a.page-link{href: '#'} 1
    %li.page-item
      %a.page-link{href: '#'} 2
    %li.page-item
      %a.page-link{href: '#'} 3
    %li.page-item
      %a.page-link{href: '#'} ${3:Next}
